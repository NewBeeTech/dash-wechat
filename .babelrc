{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-flow-strip-types",
    "syntax-jsx",
    "transform-es2015-modules-commonjs",
    "transform-decorators-legacy",
    ["import", { "style": "css", "libraryName": "antd-mobile" }]
  ]
}
